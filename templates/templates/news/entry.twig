{% extends "./layouts/base.twig" %}

{% block content %}
  <article>
    {% include "./partials/section/section.twig" with {title: entry.title, background: 'dark'} %}
    {% embed "./partials/section/section.twig" with { aside: true} %}
      {% block aside %}
        {% include "./components/aside/aside.twig" with {entryUrl: entry.url} %}
      {% endblock %}
      {% block content %}
        {% include "./components/contentBlocks/contentBlocks.twig" with {contentBlocks: entry.contentBlocks} %}
      {% endblock %}
    {% endembed %}
  </article>
{% endblock %}